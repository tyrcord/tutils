{
  "name": "@tutils/subx",
  "version": "0.3.3",
  "description": "RxJS subscriptions management",
  "author": "Tyrcord Inc. <dev@tyrcord.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": "https://github.com/tyrcord/tutils/tree/master/packages/subx",
  "bugs": "https://github.com/tyrcord/tutils/issues",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "NODE_ENV=test mocha",
    "coverage": "nyc npm test",
    "codecov": "nyc merge .nyc_output ./coverage/coverage.json"
  },
  "peerDependencies": {
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
